<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
        $("a").click(function {
        e.preventDefault();
        $("a").addClass("active").not(this).removeClass("active");
        });
        });
    </script>
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>AutoMapper Presentation</h1>
            </div>
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("Manual Mapping", "ManualMapping", "Home")</li>
                    <li>@Html.ActionLink("Normal Mapping", "AuthorList", "Home")</li>
                    <li>@Html.ActionLink("Flattening", "SalesList", "Home")</li>
                    <li>@Html.ActionLink("Projection", "AuthorListWithProjection", "Home")</li>
                    <li>@Html.ActionLink("NestedMapping", "EmployeeJobWithNestedMapping", "Home")</li>
                    <li>@Html.ActionLink("CustomTypeConverter", "CustomTypeConverter", "Home")</li>
                    <li>@Html.ActionLink("ResolveUsing-1", "ResolveUsing", new { controller = "Home", resolvetype = 1 })</li>
                    <li>@Html.ActionLink("ResolveUsing-2", "ResolveUsing", new { controller = "Home", resolvetype = 2 })</li>
                    <li>@Html.ActionLink("ResolveUsing-3", "ResolveUsing", new { controller = "Home", resolvetype = 3 })</li>
                    
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
        </div>
    </div>
</body>
</html>
